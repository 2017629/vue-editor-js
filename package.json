  "files": [
    "dist/vue-editor-js.common.js",
    "dist/vue-editor-js.umd.min.js",
    "dist/vue-editor-js.umd.js",
    "dist/vue-editor-js.esm.js",
    "src"
  ],
  "dependencies": {
    "core-js": "^2.6.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-cli-plugin-p11n": "^0.2.1",
    "vue-template-compiler": "^2.5.21",
    "vue": "^2.6.6"
